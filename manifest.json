{
  "name": "Vault Key: Borderlands™ 3 Reward Redeemer",
  "version": "1.1",
  "description": "Automatically redeem reward codes for in-game Borderlands™ 3 Loot",
  "permissions": [
    "activeTab", 
    "storage",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "notifications",
    "https://api.2k.com/",
    "https://2kgames.crowdtwist.com/",
    "http://orcz.com/",
    "https://www.reddit.com/"
  ],
  "background": {
    "scripts": [ 
      "redeemer.js",
      "context.js"
    ],
    "persistent": true
  },
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/vault-key-16.png",
      "32": "images/vault-key-32.png",
      "48": "images/vault-key-48.png",
      "128": "images/vault-key-128.png",
      "256": "images/vault-key-256.png",
      "512": "images/vault-key-512.png"
    }
  },
  "icons": {
    "16": "images/vault-key-16.png",
    "32": "images/vault-key-32.png",
    "48": "images/vault-key-48.png",
    "128": "images/vault-key-128.png",
    "256": "images/vault-key-256.png",
    "512": "images/vault-key-512.png"
  },
  "manifest_version": 2
}
